  "scripts": {
    "build:dev": "cp ./server/tsconfig.dev.json ./server/tsconfig.json && npm run build && cp ./server/config.dev.yml ./server/build-server/config.yml",
    "build:beta": "cp ./server/tsconfig.beta.json ./server/tsconfig.json && npm run build && cp ./server/config.beta.yml ./server/build-server/config.yml",
    "build:production": "cp ./server/tsconfig.production.json ./server/tsconfig.json && npm run build && cp ./server/config.production.yml ./server/build-server/config.yml",
    "prebuild": "rm -rf server/build-server",
    "build": "tsc -p server",
    "postbuild": "cp -r ./server/src/views ./server/build-server/views",
    "start:dev": "cp ./server/tsconfig.dev.json ./server/tsconfig.json && cp ./server/config.dev.yml ./server/config.yml && npm run start",
    "start:beta": "cp ./server/tsconfig.beta.json ./server/tsconfig.json && cp ./server/config.beta.yml ./server/config.yml && npm run start",
    "start:production": "cp ./server/tsconfig.production.json ./server/tsconfig.json && cp ./server/config.production.yml ./server/config.yml && npm run start",
    "start": "npx nodemon --watch server --ext ts --delay 1 --exec 'ts-node' --project server/tsconfig.json server/src/main.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
